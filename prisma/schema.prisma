generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Creature{
  id              Int         @id @default(autoincrement())
  name            String      @unique
  description     String?
  lastSee         String
  countLastSee    Int         @default(1)
  extinct         Boolean     @default(false)
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
}
